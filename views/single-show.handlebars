<article class="single-show">
    <div class="show-info">
        <h2>{{show.bandname}}</h2>
        <ol>
            <li>{{show.genre}}</li>
            <li>{{show.youtube_id}}</li>
            <li>{{show.show_location}}</li>
            <li>{{show.show_time}}</li>
            <li>{{show.show_date}}</li>
            <li>{{show.user_id}}</li>
            <li>{{show.privacy}}</li>
            <li><img src="{{show.img}}" alt="band image" width="250"></li>
