<article class="show">
    <div class="bandname">
        <a href="/show/{{id}}">{{bandname}}</a>
    </div>
    <div>
        <h1> From the Partial</h1>
        At {{show_time}}
    </div>

</article>

<article class="card">

    <div class="card-body">
        <img src="{{img}}" alt="band image" width="250">

        <div class="card-header">
            <a class="bandname" href="/show/{{id}}">{{bandname}}</a>
            {{!-- actually use the handlebars --}}
            <p>Sept 24</p>
        </div>
        <p>{{show_location}}</p>
        <p>Time: {{show_time}} </p>
        <a class="show-details" href="/show/{{id}}">view show details....</a>
    </div>



</article>
<script src="../js/postcomment.js"></script>